{
    "name": "nimbusec/nimbusec-php",
    "description": "The official Nimbusec API PHP Client",
    "homepage": "https://nimbusec.com",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "~6.0.0",
        "webmozart/path-util": "~2.0.0",
        "composer/ca-bundle": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "authors": [
        {
            "name": "Nimbusec",
            "email": "office@nimbusec.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nimbusec\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nimbusec\\Tests\\": "tests/"
        }
    },
    "scripts":{
        "test": "phpunit"
    }
}
