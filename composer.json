{
    "name": "nimbusec/nimbusec-php",
    "type": "library",
    "description": "The official Nimbusec API PHP Client",
    "keywords": ["sdk", "client", "nimbusec", "rest", "api"],
    "homepage": "https://nimbusec.com",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "~6.0.0",
        "webmozart/path-util": "~2.0.0",
        "composer/ca-bundle": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "authors": [
        {
            "name": "Nimbusec",
            "email": "office@nimbusec.com",
            "homepage": "https://nimbusec.com",
            "role": "developer"
        },
        {
            "name": "Alen Kocaj",
            "email": "alen.kocaj@posteo.at",
            "homepage": "https://github.com/rathalos64",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cumulodev/nimbusec-PHP"
        },
    ],
    "autoload": {
        "psr-4": {
            "Nimbusec\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nimbusec\\Tests\\": "tests/"
        }
    },
    "scripts":{
        "test": "phpunit --verbose"
    }
}
